package Multithreding;
class Program5 extends Thread {
    public void run() {
        System.out.println(getName() + " running");
    }
    public static void main(String[] args) throws Exception {
        JoinDemo t1 = new JoinDemo();
        JoinDemo t2 = new JoinDemo();

        t1.start();
        t1.join();
        t2.start();
    }
}
