package Inheritance;
class Fruit1 {
 String name;
 String color;
}
class Apple1 extends Fruit1 {
 String taste;
}
class Banana1 extends Fruit1 {
 String size;
}
public class Program21{
 public static void main(String[] args) {
     Apple1 a = new Apple1();
     a.name = "Apple";      
     a.color = "Red";    
     a.taste = "Sweet";    
     Banana1 b = new Banana1();
     b.name = "Banana";    
     b.color = "Yellow";    
     b.size = "Large"; 
     System.out.println("Apple Details:");
     System.out.println(a.name + " " + a.color + " " + a.taste);
     System.out.println();
     System.out.println("Banana Details:");
     System.out.println(b.name + " " + b.color + " " + b.size);
 }
}
