package Collections;
import java.util.*;
public class Launch2 {
	 static void main(String args[]) {
		 ArrayList <Integer> al=new ArrayList<Integer>();
		 al.add(100);
		 al.add(50);
		 al.add(150);
		 System.out.println(al);
		 al.add(2,4343);
		 System.out.println(al);
		 al.addFirst(111);
		 System.out.println(al);
		 al.addLast(999);
		 System.out.println(al);
		 ArrayList al2=new ArrayList();
		 al2.add(10);
		 al2.add(20);
		 al2.add(30);
		 al2.add(40);
		 al2.add(50);
		 System.out.println(al2);
	 }
}
