package Collections;
import java.util.*;
class Cricketer{
	String name;
	int runs;
	int wickets;
	int matches;
	int catches;
	public Cricketer(String name, int runs, int wickets, int matches, int catches) {
		super();
		this.name = name;
		this.runs = runs;
		this.wickets = wickets;
		this.matches = matches;
		this.catches = catches;
	}
	@Override
	public String toString() {
		return name+"---"+runs+"---"+wickets+"---"+matches+"---"+catches;
	}
	
}
public class Launch10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub]
		Cricketer c1=new Cricketer("sachin",1800,235,300,500);
		Cricketer c2=new Cricketer("Dhoni",1000,300,250,500);
		Cricketer c3=new Cricketer("virat",13000,500,300,1000);
		ArrayList al=new ArrayList();
		al.add(c1);
		al.add(c2);
		al.add(c3);
		System.out.println(al);

	}

}
