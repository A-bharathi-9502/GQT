package Patterns;
import java.util.*;
public class Program20 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		System.out.println("Enter size");
		int n=s.nextInt();
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=n;j++) {
				System.out.print("$");
			}
			for(int k=1;k<=i;k++) {
				System.out.print("%");
			}
		}
	}

}
