package threads;
class Microsoft extends Thread{
	public void run() {
		if(Thread.currentThread().getName().equals("typing")) {
			typing();
		}
		else if(Thread.currentThread().getName().equals("spellcheck")) {
			specllcheck();
		}
		else {
			saving();
		}
	}

	private void typing() {
		try {
			for(int i=1;i<=5;i++) {
				System.out.println("typing...");
				Thread.sleep(300);
			}
		}
		catch(Exception e) {
			e.printStackTrace();
		}
	}
	
}
public class Launch1 {

}
